public class PALINDROME {

	public static void main(String[] args) {
		int n=525,temp,rev=0,rem;
		 temp=n;
		while(n>0)
		{
			rem=n%10;
			rev=(rev*10)+rem;
			n=n/10;
			
		}
		if(temp==rev)
		{
			System.out.println("palindrome");
		}
		else
		{
			System.out.println("not palindrome");
		}


        	
	}

}
